def arrayColoring(array):
    if (sum(array) % 2 == 0):
        return "YES"
    else:
        return "NO"


if __name__ == "__main__":
    testCases = int(input())

    for i in range(testCases):
        arrayLength = int(input())
        array = [int(x) for x in input().split()]
        print(arrayColoring(array))